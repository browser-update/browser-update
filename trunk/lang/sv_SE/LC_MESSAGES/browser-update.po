msgstr "Webbläsaruppdateringar"

#: stat.php:60
#, php-format
msgid "<strong class=\"number\">%d</strong> visitors have already upgraded their browser."
msgstr "<strong class=\"number\">%d</strong> besökare har uppdaterat sin webbläsare."

