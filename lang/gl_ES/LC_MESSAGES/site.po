#, php-format
msgid "<b>%s</b> sites are using this notification"
msgstr ""
"<strong class=\"number\">%d</strong> sitios web están usando o script de "
"Browser-Update.org."

#: stat.php:22
msgid "Browser updates"
msgstr "Actualizacións de navegador web"

#: stat.php:60
#, php-format
msgid "<b>%s</b> visitors have already updated their browser"
msgstr ""
"<strong class=\"number\">%d</strong> visitantes xa actualizaron o seu "
"navegador web."

